 "html": "\r\n\r\n<style type=\"text/css\">\r\n.address {\tfont-family:Arial, Helvetica, sans-serif;\r\n\tfont-size:13px;\r\n}\r\n.address-heading {\tfont-family:Arial, Helvetica, sans-serif;\r\n\tfont-size:15px;\r\n\tfont-weight:bold;\r\n}\r\n.address-heading-center {\tfont-family:Arial, Helvetica, sans-serif;\r\n\tfont-size:15px;\r\n\tfont-weight:bold;\r\n\ttext-align:center;\r\n}\r\n.heading {\tfont-family: \"Arial Black\", Gadget, sans-serif;\r\n\tfont-size: 20px;\r\n\tfont-weight: bold;\r\n\ttext-align:center;\r\n\tcolor:076ba0;\r\n}\r\n.normal-content {\t\tfont-family:Arial, Helvetica, sans-serif;\r\n\tfont-size:14px;\r\n\ttext-align:left;\r\n}\r\n.normal-content-white {\t\tfont-family:Arial, Helvetica, sans-serif;\r\n\tfont-size:14px;\r\n\ttext-align:left;\r\n\tfont-weight:bold;\r\n\ttext-align:center;\r\n\tcolor:#FFF;\r\n}\r\n.normal-content-center {\t\tfont-family:Arial, Helvetica, sans-serif;\r\n\tfont-size:14px;\r\n\ttext-align:center;\r\n}\r\n\r\n.normal-content-bold-center {\t\tfont-family:Arial, Helvetica, sans-serif;\r\n\tfont-size:14px;\r\n\ttext-align:center;\r\n\tfont-weight:bold;\r\n}\r\n.normal-content-bold-left {\t\tfont-family:Arial, Helvetica, sans-serif;\r\n\tfont-size:14px;\r\n\ttext-align:left;\r\n\tfont-weight:bold;\r\n}\r\n.normal-content-bold-right {\t\tfont-family:Arial, Helvetica, sans-serif;\r\n\tfont-size:14px;\r\n\ttext-align:right;\r\n\tfont-weight:bold;\r\n}\r\n#border-bottom {\t\r\nborder-bottom:solid 3px;\r\nborder-bottom-color:#076ba0;\r\n\r\n\r\n}\r\n#border-bottom-light {\t\r\nborder-bottom:solid 2px;\r\nborder-bottom-color:#076ba0;\r\n\r\n\r\n}\r\n#border-bottom-grey {\t\r\nborder-bottom:solid 2px;\r\nborder-bottom-color:#CCC;\r\n\r\n\r\n}\r\n#border-bottom-right {\t\r\nborder-right:solid 2px;\r\nborder-right-color:#076ba0;\r\n\r\n\r\n}\r\n</style>\r\n<table width=\"100%\">\r\n  <tr>\r\n    <td><img src=\"https://erp.sahexports.com/files/SAH_logo.png\" width=\"200px\"><br>\r\n      <span class=\"address-heading\">(HOME TEXTILE DIVISION)</span><br><span class=\"address\">Plot # :4,5,6,13,14,15, Sector B-IX, <br>\r\nKarachi Export Processing Zone,<br>\r\nKarachi 75150 Pakistan.<br>\r\nPh 021-35080157<br>\r\nWebsite : www.sahent.com</span></td>\r\n    <td width=\"20%\">&nbsp;</td>\r\n  </tr>\r\n  <tr>\r\n    <td colspan=\"2\" align=\"center\" id=\"border-bottom\"><span class=\"heading\">General Item Issuance</span></td>\r\n  </tr>\r\n</table>\r\n<br>\r\n<table width=\"100%\">\r\n\t<tr>\r\n\t\t<td class=\"normal-content\"><strong>User :</strong>{{frappe.db.get_value('User', doc.user, 'first_name') or ''}} <br><strong>By Hand :</strong> {{doc.by_hand}}</td>\r\n\t\t<td class=\"normal-content\"> <strong>Date : </strong>{{doc.get_formatted('date')}}</td>\r\n\t</tr>\r\n\r\n</table>\r\n<br>\r\n<table width=\"100%\" border=\"1\" style=\"border-color: #F2F2F2;\">\r\n\t<tr style=\"border: 1px solid #807c7c; background: #076ba0;\">\r\n\t\t<td class=\"normal-content-white\">No</td>\r\n\t\t<td class=\"normal-content-white\">Part Name</td>\r\n\t\t<td class=\"normal-content-white\">Qty</td>\r\n\t\t<td class=\"normal-content-white\">Unit</td>\r\n\t\t<td class=\"normal-content-white\">Department</td>\r\n\r\n\t</tr>\r\n\t{%- for item in doc.general_item_issuance_ct -%}\r\n\t<tr style=\"border: 1px solid #807c7c;\"> \r\n\t\t<td class=\"normal-content-center\">{{item.idx}}</td>\r\n\t\t<td class=\"normal-content-center\">{{item.part_name}}</td>\r\n\t\t<td class=\"normal-content-center\">{{item.get_formatted('qty')}}</td>\r\n\t\t<td class=\"normal-content-center\">{{item.get_formatted('unit')}}</td>\r\n\t\t<td class=\"normal-content-center\">{{item.department}}</td>\r\n\r\n\t</tr>{%- endfor -%}\r\n\t</table>\r\n    \r\n\r\n<br> \r\n\r\n{%if doc.machine_part_returned%}\r\n<table width=\"100%\">\r\n  <tr>\r\n    <td id=\"border-bottom\" align=\"center\"><span class=\"heading\">General Item Return</span></td>\r\n  </tr>\r\n</table>\r\n<br>\r\n\r\n<table width=\"100%\" border=\"1\" style=\"border-color: #F2F2F2;\">\r\n\t<tr style=\"border: 1px solid #807c7c; background: #076ba0;\">\r\n\t\t<td class=\"normal-content-white\" width=\"5%\">No</td>\r\n\t\t<td class=\"normal-content-white\" width=\"65%\">Part Name</td>\r\n\t\t<td class=\"normal-content-white\" width=\"15%\">Qty</td>\r\n\t\t<td class=\"normal-content-white\" width=\"15%\">Unit</td>\r\n\t</tr>\r\n\t{%- for item in doc.machine_part_returned -%}\r\n\t<tr> \r\n\t\t<td class=\"normal-content-center\">{{item.idx}}</td>\r\n\t\t<td class=\"normal-content-center\">{{item.part_name}}</td>\r\n\t\t<td class=\"normal-content-center\">{{item.get_formatted('qty')}}</td>\r\n\t\t<td class=\"normal-content-center\">{{item.get_formatted('unit')}}</td>\r\n\t</tr>{%- endfor -%}\r\n\t</table>\r\n\r\n{%endif%}",
